!!!
%html
  %head
    %title Chrisbim
    = stylesheet_link_tag    "application", :media => "all"
    = javascript_include_tag "application"
    = csrf_meta_tags
  %body
    = yield
    #footer
      %p= current_user.inspect      
      %p= current_user_session.inspect
      - if current_user
        = link_to 'Logout', logout_path
      - else
        = link_to 'Login', login_path
      %p= params.inspect
